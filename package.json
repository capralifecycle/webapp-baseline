{
  "name": "webapp-baseline",
  "version": "0.0.1",
  "description": "Baseline for creating React+Typescript web apps",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve",
    "start:cypress": "DANGEROUSLY_DISABLE_HOST_CHECK=true webpack serve",
    "build": "webpack --env production",
    "build:measure": "MEASURE=true webpack --env production",
    "build:analyze": "ANALYZE=true webpack --env production",
    "build:ci": "npm run build:analyze",
    "test": "jest --coverage src",
    "test:watch": "jest src --watch",
    "test:update-snapshots": "jest -u",
    "test:cypress": "cypress run",
    "test:cypress:docker": "cypress install && npm run test:cypress",
    "test:cypress:open": "cypress open",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@cypress/browserify-preprocessor": "3.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "@types/jest": "27.0.2",
    "@types/jest-each": "24.3.0",
    "@types/mini-css-extract-plugin": "2.4.0",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-router-dom": "5.3.2",
    "@types/speed-measure-webpack-plugin": "1.3.4",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-dev-server": "4.3.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "autoprefixer": "10.4.0",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.5.0",
    "cypress": "8.7.0",
    "cypress-plugin-snapshots": "1.4.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "git-revision-webpack-plugin": "5.0.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "13.0.1",
    "jest": "27.3.1",
