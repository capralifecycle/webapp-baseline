{
  "name": "webapp-baseline",
  "version": "0.0.1",
  "description": "Baseline for creating React+Typescript web apps",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve",
    "start:cypress": "DANGEROUSLY_DISABLE_HOST_CHECK=true webpack serve",
    "build": "webpack --env production",
    "build:measure": "MEASURE=true webpack --env production",
    "build:analyze": "ANALYZE=true webpack --env production",
    "build:ci": "npm run build:analyze",
    "test": "jest --coverage src",
    "test:watch": "jest src --watch",
    "test:update-snapshots": "jest -u",
    "test:cypress": "cypress run",
    "test:cypress:docker": "cypress install && npm run test:cypress",
    "test:cypress:open": "cypress open",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@cypress/browserify-preprocessor": "3.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@types/jest": "26.0.23",
    "@types/jest-each": "24.3.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-router-dom": "5.1.7",
    "@types/speed-measure-webpack-plugin": "1.3.3",
    "@types/webpack-bundle-analyzer": "4.4.0",
    "@types/webpack-dev-server": "3.11.4",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "autoprefixer": "10.2.6",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.2.6",
    "cypress": "7.6.0",
    "cypress-plugin-snapshots": "1.4.4",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "git-revision-webpack-plugin": "5.0.0",
    "html-webpack-plugin": "5.3.2",
    "http-server": "0.12.3",
    "jest": "27.0.5",
    "jest-each": "27.0.2",
    "postcss": "8.3.5",
    "postcss-loader": "6.1.0",
    "prettier": "2.3.2",
    "react-refresh": "0.10.0",
    "source-map-loader": "3.0.0",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "2.0.0",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.4",
    "url-loader": "4.1.1",
    "wait-on": "5.3.0",
    "webpack": "5.40.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
