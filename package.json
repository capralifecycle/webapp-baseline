{
  "name": "webapp-baseline",
  "version": "0.0.1",
  "description": "Baseline for creating React+Typescript web apps",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --env.production",
    "build:measure": "MEASURE=true webpack --env.production",
    "build:analyze": "ANALYZE=true webpack --env.production",
    "test": "jest --coverage src",
    "test:watch": "jest src --watch",
    "test:e2e": "testcafe chrome e2e/tests/**/*.test.ts",
    "test:e2e:jenkins": "testcafe --app './scripts/serve-dist.sh' chrome:headless e2e/tests/**/*.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/jest-each": "24.3.0",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "autoprefixer": "10.0.1",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.3.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "git-revision-webpack-plugin": "3.0.6",
    "html-webpack-plugin": "4.5.0",
    "http-server": "0.12.3",
    "jest": "26.5.3",
    "jest-each": "26.5.2",
    "jest-sonar-reporter": "2.0.0",
    "postcss": "8.1.1",
    "postcss-loader": "4.0.4",
    "prettier": "2.1.2",
    "source-map-loader": "1.1.1",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-loader": "1.3.0",
    "testcafe": "1.9.4",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.5",
    "typescript": "4.0.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
