{
  "name": "webapp-baseline",
  "version": "0.0.1",
  "description": "Baseline for creating React+Typescript web apps",
  "main": "index.tsx",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "start:cypress": "DANGEROUSLY_DISABLE_HOST_CHECK=true webpack serve",
    "build": "webpack --env production",
    "build:measure": "MEASURE=true webpack --env production",
    "build:analyze": "ANALYZE=true webpack --env production",
    "build:ci": "npm run build:analyze",
    "test": "jest --coverage src",
    "test:watch": "jest src --watch",
    "test:update-snapshots": "jest -u",
    "test:cypress": "cypress run",
    "test:cypress:docker": "cypress install && npm run test:cypress",
    "test:cypress:open": "cypress open",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "devDependencies": {
    "@cypress/browserify-preprocessor": "3.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@types/jest": "27.4.1",
    "@types/jest-each": "24.3.0",
    "@types/mini-css-extract-plugin": "2.5.0",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/react-router-dom": "5.3.3",
    "@types/speed-measure-webpack-plugin": "1.3.4",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-dev-server": "4.5.1",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.7.1",
    "cypress": "9.2.1",
    "cypress-plugin-snapshots": "1.4.4",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "git-revision-webpack-plugin": "5.0.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.1.0",
    "jest": "27.5.1",
    "jest-each": "27.5.1",
    "mini-css-extract-plugin": "2.6.0",
    "postcss": "8.4.12",
