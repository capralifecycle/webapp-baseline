{
  "name": "webapp-baseline",
  "version": "0.0.1",
  "description": "Baseline for creating React+Typescript web apps",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --env production",
    "build:measure": "MEASURE=true webpack --env production",
    "build:analyze": "ANALYZE=true webpack --env production",
    "test": "jest --coverage src",
    "test:watch": "jest src --watch",
    "test:e2e": "testcafe chrome e2e/tests/**/*.test.ts",
    "test:e2e:jenkins": "testcafe --app './scripts/serve-dist.sh' chrome:headless e2e/tests/**/*.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/jest-each": "24.3.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "autoprefixer": "10.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "git-revision-webpack-plugin": "3.0.6",
    "html-webpack-plugin": "4.5.1",
    "http-server": "0.12.3",
    "jest": "26.6.3",
    "jest-each": "26.6.2",
    "jest-sonar-reporter": "2.0.0",
    "postcss": "8.2.2",
    "postcss-loader": "4.1.0",
    "prettier": "2.2.1",
    "source-map-loader": "2.0.0",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-loader": "2.0.0",
    "testcafe": "1.10.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.14",
    "typescript": "4.1.3",
    "url-loader": "4.1.1",
    "webpack": "5.11.1",
    "webpack-bundle-analyzer": "4.3.0",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "3.11.1"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
