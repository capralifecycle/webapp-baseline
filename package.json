    "awesome-typescript-loader": "5.2.1",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.2",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "testcafe": "0.23.2",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6",
